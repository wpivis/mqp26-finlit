{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/v2.2.0/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "MQP Prototype Demo",
    "version": "pilot",
    "authors": [
      "MQP Team"
    ],
    "date": "2025-11-04",
    "description": "A demo test for ReVisit implementation of prototype.",
    "organizations": [
      "University of Utah",
      "WPI",
      "University of Toronto"
    ]
  },
  "uiConfig": {
    "contactEmail": "contact@revisit.dev",
    "helpTextPath": "mqp-prototype/assets/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": true,
    "enumerateQuestions": true
  },
  "components": {
    "introduction": {
      "type": "markdown",
      "path": "mqp-prototype/assets/introduction.md",
      "response": []
    },
    "pre": {
      "type": "questionnaire",
      "response": [
        {
          "id": "textField2",
          "type": "textOnly",
          "location": "aboveStimulus",
          "prompt": "# Questions before seeing prototype\n\n Answer below",
          "restartEnumeration": true
        },
        {
          "id": "q-current-methods",
          "prompt": "How do you currently track your finances or savings?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-used-apps",
          "prompt": "What apps or tools have you used for money in the past? What did you like or dislike?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-features",
          "prompt": "What features do you expect in finance tracking/saving apps?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        }
      ]
    },
    "figmaTask": {
      "type": "website",
      "path": "https://embed.figma.com/proto/cPBlfjySSUhw069Qlo3R1a/MQP-Prototype?page-id=1%3A91&node-id=170-2&starting-point-node-id=170%3A2&scaling=min-zoom&content-scaling=fixed&embed-host=share",
      "instruction": "Please interact with the prototype below to complete the task. Zoom out (Ctrl -) if needed",
      "response": []
    },
    "demo": {
      "type": "questionnaire",
      "response": [
        {
          "id": "header",
          "type": "textOnly",
          "location": "aboveStimulus",
          "prompt": "# Questions While Seeing Prototype\n\n Answer below",
          "restartEnumeration": true
        },
        {
          "id": "q-confusing",
          "prompt": "Is there anything on this page that feels confusing or out of place?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-missing",
          "prompt": "What, if anything, is missing from this screen?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        }
      ]
    },
    "post": {
      "type": "questionnaire",
      "response": [
        {
          "id": "textField2",
          "type": "textOnly",
          "location": "aboveStimulus",
          "prompt": "# Questions after seeing prototype\n\n Answer below",
          "restartEnumeration": true
        },
        {
          "id": "q-feature-satisfaction",
          "prompt": "Rate your satisfaction from 1 (Highly Unsatisfied) to 5 (Highly Satisfied) for the following items.",
          "secondaryText": "A simple matrix of Likert responses with predefined options for satisfaction.",
          "location": "aboveStimulus",
          "type": "matrix-radio",
          "answerOptions": "satisfaction5",
          "questionOptions": [
            "Overall Prototype",
            "Goals Feature",
            "Streak Feature"
          ]
        },
        {
          "id": "q-like-dislike",
          "prompt": "What did you like the most about the prototype? What did you like the least?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-most-frustrating",
          "prompt": "What was the most frustrating part of the experience, if any?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-streak-feedback",
          "prompt": "If you could change one thing about the streak feature, what would it be?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-goals-feedback",
          "prompt": "If you could change one thing about the goal feature, what would it be?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        },
        {
          "id": "q-additional-comments",
          "prompt": "Is there anything else you'd like to share that has not been asked?",
          "location": "aboveStimulus",
          "type": "longText",
          "placeholder": "Enter your long comments here"
        }
      ]
    }
  },
  "sequence": {
    "order": "fixed",
    "components": [
      "introduction",
      "pre",
      "figmaTask",
      "demo",
      "post"
    ]
  }
}
